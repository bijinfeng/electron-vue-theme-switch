{
  "name": "electron-vue-vite",
  "version": "2.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Electron + Vue theme switch demo",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "vue",
    "theme"
  ],
  "debug": {
