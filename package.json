    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  },
  "dependencies": {
    "add": "^2.0.6",
    "electron-store": "^8.1.0",
    "flowbite": "^2.0.0",
    "pnpm": "^8.10.0"
  }
